using System;
using System.Collections.Generic;
using System.Linq;
using System.Web;

namespace ContentModel1._5.Common
{
    public class ModuleField
    {
        /// <summary>
        /// 模型ID
        /// </summary>
        public int FieldID { get; set; }
        /// <summary>
        /// 模型编码
        /// </summary>
        public string ModelCode { get; set; }
        /// <summary>
        /// 源模型名称
        /// </summary>
        public string ModelName { get; set; }
        /// <summary>
        /// 目标模型名称
        /// </summary>
        public string TargetModelName { get; set; }
        /// <summary>
        /// 字段编码
        /// </summary>
        public string Code { get; set; }
        /// <summary>
        /// 字段名称
        /// </summary>
        public string Name { get; set; }
        /// <summary>
        /// 字段别名
        /// </summary>
        public string Nick { get; set; }
        /// <summary>
        /// 字段提示
        /// </summary>
        public string Tooltip { get; set; }
        /// <summary>
        /// 字段描述
        /// </summary>
        public string Remark { get; set; }
        /// <summary>
        /// 错误提示
        /// </summary>
        public string ErrorText { get; set; }
        /// <summary>
        /// 文本框为空的提示
        /// </summary>
        public string NullText { get; set; }
        /// <summary>
        /// 字段宽度，图片宽度
        /// </summary>
        public short Width { get; set; }
        /// <summary>
        /// 字段高度，图片高度
        /// </summary>
        public short Height { get; set; }
        /// <summary>
        /// 输入最大长度
        /// </summary>
        public short MaxLength { get; set; }
        /// <summary>
        /// 字段分组编码
        /// </summary>
        public string GroupCode { get; set; }
        /// <summary>
        /// 字段分组名称
        /// </summary>
        public string GroupName { get; set; }
        /// <summary>
        /// 字段显示顺序
        /// </summary>
        public short DisplayOrder { get; set; }
        /// <summary>
        /// 是否必填
        /// </summary>
        public bool IsRequired { get; set; }
        /// <summary>
        /// 是否在搜索项中
        /// </summary>
        public bool IsAllowSearch { get; set; }
        /// <summary>
        /// 是否只读
        /// </summary>
        public bool IsReadOnly { get; set; }
        /// <summary>
        /// 是否可见
        /// </summary>
        public bool IsVisible { get; set; }
        /// <summary>
        /// 默认值
        /// </summary>
        public object DefaultValue { get; set; }
        /// <summary>
        /// 最大值
        /// </summary>
        public object MaxValue { get; set; }
        /// <summary>
        /// 最小值
        /// </summary>
        public object MinValue { get; set; }
        /// <summary>
        /// 当前码
        /// </summary>
        public object CurrentCode { get; set; }
        /// <summary>
        /// 当前值
        /// </summary>
        public object CurrentValue { get; set; }
        /// <summary>
        /// 代码分类
        /// </summary>
        public string CodeCat { get; set; }
        /// <summary>
        /// 格式、排列方式、图片文件类型
        /// </summary>
        public string DisplayFormat { get; set; }
        /// <summary>
        /// 是否加水印
        /// </summary>
        public bool HasWaterMark { get; set; }
        /// <summary>
        /// 加何种水印
        /// </summary>
        public string WaterMarkType { get; set; }
        /// <summary>
        /// 图片、文件规格大小
        /// </summary>
        public int ImageSize { get; set; }
        /// <summary>
        /// 是否多选（选择用户，选择代码，选择机构，选择角色）
        /// </summary>
        public bool SingleOrMultiple { get; set; }
        /// <summary>
        /// 字段类型
        /// </summary>
        public FieldType FieldType { get; set; }
    }
}